{
  "name": "basic-auth-app",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "start": "node ./bin/www.js",
    "start:with:db": "SET DBPORT=65535 && node ./bin/www.js",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "njwt": "^1.0.0",
    "monk": "7.3.3"
  },
  "devDependencies": {}
}
