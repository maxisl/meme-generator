{
  "name": "mememuc-launcher",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm start --prefix ../mongoserver\" \"npm start --prefix ../node-backend\" \"npm start --prefix ../react-frontend\"",
    "startdev": "concurrently \"npm run startdev --prefix ../node-backend\" \"npm start --prefix ../react-frontend\"",
    "serverdev": "npm run startdev --prefix ../node-backend",
    "fullstackdev": "concurrently \"npm run startdev --prefix ../node-backend\" \"npm run dev --prefix ../react-frontend\"",
    "installall": "npm install && cd ../mongoserver && npm install && cd ../node-backend && npm install && cd ../react-frontend && npm install",
    "postinstall": "npm run mongo:restore",
    "mongo:restore": "mongorestore --db omm-2223 --drop ../mongoserver/mongobackup/dump/omm-2223"
  },
  "keywords": [],
  "author": "LMU Mediainformatics Group",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.5.0",
    "swagger-ui-express": "^4.6.0"
  }
}
